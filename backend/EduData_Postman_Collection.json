            "body": { "mode": "raw", "raw": "{\n  \"description\": \"Actualizado desde Postman\"\n}" },
            "url": { "raw": "{{baseUrl}}/datasets/1", "host": ["{{baseUrl}}"], "path": ["datasets","1"] }
          }
        },
        {
          "name": "PATCH /datasets/:id/sync",
          "request": {
            "method": "PATCH",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": { "raw": "{{baseUrl}}/datasets/1/sync", "host": ["{{baseUrl}}"], "path": ["datasets","1","sync"] }
          }
        }
