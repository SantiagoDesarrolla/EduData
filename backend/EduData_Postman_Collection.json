            "url": { "raw": "{{baseUrl}}/reports/filter?type=regional", "host": ["{{baseUrl}}"], "path": ["reports","filter"], "query": [{ "key": "type", "value": "regional" }] }
          }
        },
        {
          "name": "GET /reports/:id",
          "request": {
            "method": "GET",
            "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
            "url": { "raw": "{{baseUrl}}/reports/1", "host": ["{{baseUrl}}"], "path": ["reports","1"] }
          }
        },
        {
          "name": "DELETE /reports/:id",
          "request": {
            "method": "DELETE",
            "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
            "url": { "raw": "{{baseUrl}}/reports/1", "host": ["{{baseUrl}}"], "path": ["reports","1"] }
          }
        }
      ]
    },
