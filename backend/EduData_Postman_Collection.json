        {
          "name": "DELETE /regions/:id",
          "request": {
            "method": "DELETE",
            "header": [],
            "url": { "raw": "{{baseUrl}}/regions/1", "host": ["{{baseUrl}}"], "path": ["regions","1"] }
          }
        }
      ]
    },
    {
      "name": "Datasets",
      "item": [
        {
          "name": "GET /datasets",
