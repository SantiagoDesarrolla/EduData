        {
          "name": "POST /datasets",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"MEN - Cobertura Educativa\",\n  \"source_url\": \"https://www.datos.gov.co/resource/educacion.json\",\n  \"description\": \"Indicadores oficiales\",\n  \"update_frequency\": \"yearly\"\n}"
