              "raw": "{\n  \"region_id\": 1,\n  \"dataset_id\": 1,\n  \"year\": 2024,\n  \"indicator_code\": \"cobertura_bruta\",\n  \"indicator_name\": \"Cobertura Bruta\",\n  \"value\": 85.4,\n  \"unit\": \"%\"\n}"
            },
            "url": { "raw": "{{baseUrl}}/indicators", "host": ["{{baseUrl}}"], "path": ["indicators"] }
          }
        },
        {
          "name": "GET /indicators?year=2024",
          "request": {
            "method": "GET",
            "header": [],
            "url": { "raw": "{{baseUrl}}/indicators?year=2024", "host": ["{{baseUrl}}"], "path": ["indicators"], "query": [{ "key": "year", "value": "2024" }] }
          }
        },
        {
          "name": "GET /indicators?dataset=1",
          "request": {
            "method": "GET",
            "header": [],
            "url": { "raw": "{{baseUrl}}/indicators?dataset=1", "host": ["{{baseUrl}}"], "path": ["indicators"], "query": [{ "key": "dataset", "value": "1" }] }
          }
        }
      ]
    },
    {
      "name": "Reports",
      "item": [
        {
          "name": "GET /reports",
          "request": {
            "method": "GET",
            "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
            "url": { "raw": "{{baseUrl}}/reports", "host": ["{{baseUrl}}"], "path": ["reports"] }
